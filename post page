  <script>

    const urls = {

      subscribe1: {

        desktop: "https://www.youtube.com/channel/UC_x5XG1OV2P6uZZ5FSM9Ttw",

        android: "intent://www.youtube.com/channel/UC_x5XG1OV2P6uZZ5FSM9Ttw#Intent;package=com.google.android.youtube;scheme=https;end",

        ios: "youtube://www.youtube.com/channel/UC_x5XG1OV2P6uZZ5FSM9Ttw"

      },

      like: {

        desktop: "https://www.youtube.com/watch?v=dQw4w9WgXcQ",

        android: "intent://www.youtube.com/watch?v=dQw4w9WgXcQ#Intent;package=com.google.android.youtube;scheme=https;end",

        ios: "youtube://www.youtube.com/watch?v=dQw4w9WgXcQ"

      },

      subscribe2: {

        desktop: "https://www.youtube.com/channel/UC29ju8bIPH5as8OGnQzwJyA",

        android: "intent://www.youtube.com/channel/UC29ju8bIPH5as8OGnQzwJyA#Intent;package=com.google.android.youtube;scheme=https;end",

        ios: "youtube://www.youtube.com/channel/UC29ju8bIPH5as8OGnQzwJyA"

      },

      watchVideo: {

        desktop: "https://www.youtube.com/watch?v=ysz5S6PUM-U",

        android: "intent://www.youtube.com/watch?v=ysz5S6PUM-U#Intent;package=com.google.android.youtube;scheme=https;end",

        ios: "youtube://www.youtube.com/watch?v=ysz5S6PUM-U"

      }

    };

    function getYouTubeLink(type) {

      const userAgent = navigator.userAgent || navigator.vendor || window.opera;

      if (/android/i.test(userAgent)) {

        return urls[type].android;

      } else if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {

        return urls[type].ios;

      } else {

        return urls[type].desktop;

      }

    }

    const btnSubscribe1 = document.getElementById("btn-subscribe-1");

    const btnLike = document.getElementById("btn-like");

    const btnSubscribe2 = document.getElementById("btn-subscribe-2");

    const btnWatchVideo = document.getElementById("btn-watch-video");

    const btnUnlock = document.getElementById("btn-unlock");

    const step2 = document.getElementById("step-2");

    const step3 = document.getElementById("step-3");

    const step4 = document.getElementById("step-4");

    const step5 = document.getElementById("step-5");

    const modalOverlay = document.getElementById("modal-overlay");

    const modalCountdown = document.getElementById("modal-countdown");

    const modalOkBtn = document.getElementById("modal-ok");

    const modalDesc = document.getElementById("modal-desc");

    // Disable right-click and keyboard shortcuts to prevent easy skipping

    document.addEventListener('contextmenu', e => e.preventDefault());

    document.addEventListener('keydown', e => {

      if (

        e.key === 'F12' ||

        (e.ctrlKey && e.shiftKey && ['I', 'J', 'C'].includes(e.key.toUpperCase())) ||

        (e.ctrlKey && e.key.toUpperCase() === 'U')

      ) {

        e.preventDefault();

      }

    });

    function enableStep(stepSection, button) {

      stepSection.classList.remove("opacity-50", "pointer-events-none");

      stepSection.setAttribute("aria-disabled", "false");

      button.disabled = false;

      button.tabIndex = 0;

    }

    function disableStep(stepSection, button) {

      stepSection.classList.add("opacity-50", "pointer-events-none");

      stepSection.setAttribute("aria-disabled", "true");

      button.disabled = true;

      button.tabIndex = -1;

    }

    function openYouTubeLink(type) {

      const url = getYouTubeLink(type);

      window.open(url, "_blank", "noopener,noreferrer");

    }

    function showModalCheck(message, onComplete) {

      modalDesc.textContent = message;

      modalOkBtn.disabled = true;

      modalOkBtn.textContent = "Wait...";

      modalOkBtn.classList.remove("enabled");

      modalCountdown.textContent = "30";

      modalOverlay.style.display = "flex";

      let timeLeft = 30;

      const interval = setInterval(() => {

        timeLeft--;

        modalCountdown.textContent = timeLeft;

        if (timeLeft <= 0) {

          clearInterval(interval);

          modalOkBtn.disabled = false;

          modalOkBtn.textContent = "Continue";

          modalOkBtn.classList.add("enabled");

          modalOkBtn.focus();

        }

      }, 1000);

      function onOkClick() {

        modalOkBtn.removeEventListener("click", onOkClick);

        modalOverlay.style.display = "none";

        onComplete();

      }

      modalOkBtn.addEventListener("click", onOkClick);

    }

    btnSubscribe1.addEventListener("click", () => {

      btnSubscribe1.disabled = true;

      openYouTubeLink("subscribe1");

      showModalCheck("The system is checking whether you have subscribed or not.", () => {

        enableStep(step2, btnLike);

      });

    });

    btnLike.addEventListener("click", () => {

      btnLike.disabled = true;

      openYouTubeLink("like");

      showModalCheck("The system is checking whether you have liked or not.", () => {

        enableStep(step3, btnSubscribe2);

      });

    });

    btnSubscribe2.addEventListener("click", () => {

      btnSubscribe2.disabled = true;

      openYouTubeLink("subscribe2");

      showModalCheck("The system is checking whether you have subscribed or not.", () => {

        enableStep(step4, btnWatchVideo);

      });

    });

    btnWatchVideo.addEventListener("click", () => {

      btnWatchVideo.disabled = true;

      openYouTubeLink("watchVideo");

      enableStep(step5, btnUnlock);

    });

    btnUnlock.addEventListener("click", () => {

      btnUnlock.disabled = true;

      alert("UNLOCK THIS POST : DEAD RAILS SCRIPT\n\nThank you for completing all steps.");

    });

    disableStep(step2, btnLike);

    disableStep(step3, btnSubscribe2);

    disableStep(step4, btnWatchVideo);

    disableStep(step5, btnUnlock);

  </script>

</body>

